{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "log4net": {
        "fileName": "Logs/app_log.txt",
        "pattern": "%date [%thread] %-5level %logger - %message%newline",
        "maxFileSize": "5MB",
        "maxBackupFiles": "10"
    }
}
